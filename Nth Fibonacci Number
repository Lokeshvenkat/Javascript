function checkFibonacci(n) {
  if (n<1) {
    console.log("Invalid");
  } else if (n==1) {
    console.log("0");
  } else if (n==2) {
    console.log("0, 1");
  } else if (n>2) {
    var sum = 0;
    var n1 = 0;
    var n2 = 1;
    for (var i = 1; i < n; i++) {
      console.log(n1);
      sum = n1+n2;
      n1 = n2;
      n2 = sum;
    }
  }
}

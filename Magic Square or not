function checkMagicSquare(n, matrix) {
    
    let sumOfColumns =0;
    let sumOfDiagonal1 =0;
    let sumOfDiagonal2 =0;

    for(let i=0;i<n;i++){
        sumOfRows+=matrix.length[i][i];
        sumOfColumns+=matrix.length[i][i];
    }
}

function sumRows(matrix){
 let sumOfRows =0;
 let sumR0 =0;
 for(let i=0;i<n;i++){
     sumR0+=matrix[i][i];
 }
 
}
